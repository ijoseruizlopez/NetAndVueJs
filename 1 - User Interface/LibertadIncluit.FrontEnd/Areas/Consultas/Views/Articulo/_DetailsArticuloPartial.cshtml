                        <label class="labelTitle">Unidad de Medida</label>
                        <label class="labelDescription">{{ArticuloSelection.UnidadMedida}}</label>
                    </div>
                    <div class="col-md-3 col-sm-3 form-group fancy-form2">
                        <label class="labelTitle">Stock en tránsito</label>
                        <label class="labelDescription">{{ArticuloSelection.StockEnTransito!=null? ArticuloSelection.StockEnTransito: 0 }}</label>
                    </div>
                    <div class="col-md-3 col-sm-3 form-group fancy-form2">
                        <label class="labelTitle">Fecha de Envío</label>
                        <label class="labelDescription">{{ArticuloSelection.FechaDeEnvio!=null? ArticuloSelection.FechaDeEnvio : "-" }}</label>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-3 col-sm-4 form-group fancy-form2">
                        <label class="labelTitle">Precio Vigente</label>
                        <label class="labelDescription">{{ArticuloSelection.PrecioVigente}}</label>
                    </div>
                    <div class="col-md-3 col-sm-4 form-group fancy-form2">
                        <label class="labelTitle">Tipo de Precio</label>
                        <label class="labelDescription">{{ArticuloSelection.TipoPrecio}}</label>
                    </div>
                    <div class="col-md-6 col-sm-4 form-group fancy-form2">
                        <label class="labelTitle">Fecha última modificación</label>
                        <label class="labelDescription">{{ArticuloSelection.FechaUltimaModificacion}}</label>
                    </div>
                </div>
            </div>


            <div class="row">
                <div class="col-md-3 col-sm-6 form-group fancy-form2" align="center">
                    <img style="width:128px; height:128px" :src="ArticuloSelection.UrlImagen"/>
                </div>
                <div class="col-md-3 col-sm-6 form-group fancy-form2" style="margin-left:inherit">
                    <div class="col-md-12 col-sm-12 form-group fancy-form2">
                        <label class="labelTitle">Medidas</label>
                        <label class="labelDescription">{{ArticuloSelection.Largo}}x{{ArticuloSelection.Ancho}}x{{ArticuloSelection.Altura}} cm</label>
                    </div>
                    <div class="col-md-12 col-sm-12 form-group fancy-form2">
                        <label class="labelTitle">Peso</label>
                        <label class="labelDescription">{{ArticuloSelection.Peso}} Kg</label>
                    </div>
                </div>
            </div>


            <div class="row pull-right">
                <div class="col-md-6 col-sm-6 form-group fancy-form2 ">
                    <button class="btn btn-sm" onclick="vm.BackSearch();" style="height: 32px; margin-bottom:5px; margin-top:20px; margin-left:25px; margin-right:50px;">
                        Cerrar
                    </button>
                </div>
            </div>
