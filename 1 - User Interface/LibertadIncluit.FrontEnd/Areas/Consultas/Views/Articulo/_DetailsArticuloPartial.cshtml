<header>  
    <div class="panel-heading pointer" v-on:click="BackSearch()" tabindex="0" v-on:keyup.enter="BackSearch()">
        <img src="~/wwwroot/assets/icons/backicon.png" width="11" height="20" />
        <strong>
            Datos del Artículo
        </strong>
    </div>

    <hr id="SepartorYellow" />

    <div class="panel panel-default">
        <div class="panel-body">
            <div class="row">
                <div class="col-md-6 col-sm-6 fancy-form">
                    <label class="labelTitle">Código Interno</label>
                    <label class="labelDescription">{{ArticuloSelection.CodigoArticulo}}</label>
                </div>
                <div class="col-md-6 col-sm-6 form-group fancy-form2">
                    <label class="labelTitle">Descripción</label>
                    <label class="labelDescription" style="font-size:16px!important; color:#212121 !important; font-weight: bold;">{{ArticuloSelection.Descripcion}}</label>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6 col-sm-6 form-group fancy-form2">
                    <label class="labelTitle">EAN</label>
                    <div class="row">
                        <div v-if="ArticuloSelection.Eans[0]!=null && ArticuloSelection.Eans[0].Activo " class="col-md-4 col-sm-12 form-group fancy-form2" >
                            <label class="labelDescription labelEanActivo" >{{ ArticuloSelection.Eans[0]==null? "" : ArticuloSelection.Eans[0].Ean==null ? "" :ArticuloSelection.Eans[0].Ean}}</label>
                        </div>
                        <div v-else class="col-md-4 col-sm-12 form-group fancy-form2">
                            <label class="labelDescription">{{ ArticuloSelection.Eans[0]==null? "" : ArticuloSelection.Eans[0].Ean==null ? "" :ArticuloSelection.Eans[0].Ean}}</label>
                        </div>

                        <div v-if="ArticuloSelection.Eans[1]!=null && ArticuloSelection.Eans[1].Activo!=null && ArticuloSelection.Eans[1].Activo" class="col-md-4 col-sm-12 form-group fancy-form2">
                            <label class="labelDescription labelEanActivo">{{ ArticuloSelection.Eans[1]==null? "" : ArticuloSelection.Eans[1].Ean==null ? "" :ArticuloSelection.Eans[1].Ean}}</label>
                        </div>
                        <div v-else class="col-md-4 col-sm-12 form-group fancy-form2">
                            <label class="labelDescription">{{ ArticuloSelection.Eans[1]==null? "" : ArticuloSelection.Eans[1].Ean==null ? "" :ArticuloSelection.Eans[1].Ean}}</label>
                        </div>

                        <div v-if="ArticuloSelection.Eans[2]!=null  && ArticuloSelection.Eans[2].Activo!=null && ArticuloSelection.Eans[2].Activo" class="col-md-4 col-sm-12 form-group fancy-form2">
                            <label class="labelDescription labelEanActivo">{{ ArticuloSelection.Eans[2]==null? "" : ArticuloSelection.Eans[2].Ean==null ? "" :ArticuloSelection.Eans[2].Ean}}</label>
                        </div>
                        <div v-else class="col-md-4 col-sm-12 form-group fancy-form2">
                            <label class="labelDescription">{{ ArticuloSelection.Eans[2]==null? "" : ArticuloSelection.Eans[2].Ean==null ? "" :ArticuloSelection.Eans[2].Ean}}</label>
                        </div>
                    </div>
                    <div class="row">
                        <div v-if="ArticuloSelection.Eans[3]!=null  && ArticuloSelection.Eans[3].Activo!=null && ArticuloSelection.Eans[3].Activo " class="col-md-4 col-sm-12 form-group fancy-form2">
                            <label class="labelDescription labelEanActivo">{{ ArticuloSelection.Eans[3]==null? "" : ArticuloSelection.Eans[3].Ean==null ? "" :ArticuloSelection.Eans[3].Ean}}</label>
                        </div>
                        <div v-else class="col-md-4 col-sm-12 form-group fancy-form2">
