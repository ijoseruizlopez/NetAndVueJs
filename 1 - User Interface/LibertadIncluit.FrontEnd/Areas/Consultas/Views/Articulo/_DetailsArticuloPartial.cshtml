                        </div>

                        <div v-if="ArticuloSelection.Eans[4]!=null  && ArticuloSelection.Eans[4].Activo!=null && ArticuloSelection.Eans[4].Activo" class="col-md-4 col-sm-12 form-group fancy-form2">
                            <label class="labelDescription labelEanActivo">{{ ArticuloSelection.Eans[4]==null? "" : ArticuloSelection.Eans[4].Ean==null ? "" :ArticuloSelection.Eans[4].Ean}}</label>
                        </div>
                        <div v-else class="col-md-4 col-sm-12 form-group fancy-form2">
                            <label class="labelDescription">{{ ArticuloSelection.Eans[4]==null? "" : ArticuloSelection.Eans[4].Ean==null ? "" :ArticuloSelection.Eans[4].Ean}}</label>
                        </div>

                        <div v-if="ArticuloSelection.Eans[5]!=null  && ArticuloSelection.Eans[5].Activo!=null && ArticuloSelection.Eans[5].Activo" class="col-md-4 col-sm-12 form-group fancy-form2">
                            <label class="labelDescription labelEanActivo">{{ ArticuloSelection.Eans[5]==null? "" : ArticuloSelection.Eans[5].Ean==null ? "" :ArticuloSelection.Eans[5].Ean}}</label>
                        </div>
                        <div v-else class="col-md-4 col-sm-12 form-group fancy-form2">
                            <label class="labelDescription">{{ ArticuloSelection.Eans[5]==null? "" : ArticuloSelection.Eans[5].Ean==null ? "" :ArticuloSelection.Eans[5].Ean}}</label>
                        </div>
                    </div>
                </div>

                <div class="col-md-6 col-sm-6 form-group fancy-form2">
                    <label class="labelTitle">Estado</label>
                    <label class="labelDescription">{{ArticuloSelection.Estado}}</label>
                </div>
            </div>

            <hr/>

            <div> 
                <div class="row">
                    <div class="col-md-12 col-sm-12 form-group fancy-form2">
                        <label class="labelTitle">Estadístico</label>
                    </div>
                </div>

                <div class="row" align="center">
                    <div class="col-md-2  col-sm-12 form-group fancy-form2">
                        <label class="labelDescription">{{ArticuloSelection.Grupo.Id}} - {{ArticuloSelection.Grupo.Descripcion}}</label>
                    </div>
                    <div class="col-md-2 col-sm-12 form-group fancy-form2">
                        <label class="labelDescription">{{ArticuloSelection.Sector.Id}} - {{ArticuloSelection.Sector.Descripcion}}</label>
                    </div>
                    <div class="col-md-2 col-sm-12 form-group fancy-form2">
                        <label class="labelDescription">{{ArticuloSelection.Familia.Id}} - {{ArticuloSelection.Familia.Descripcion}}</label>
                    </div>
                    <div class="col-md-2 col-sm-12  form-group fancy-form2">
                        <label class="labelDescription">{{ArticuloSelection.SubFamilia.Id}} - {{ArticuloSelection.SubFamilia.Descripcion}}</label>
                    </div>
                    <div class="col-md-2 col-sm-12 form-group fancy-form2">
                        <label class="labelDescription">{{ArticuloSelection.Categoria.Id}} - {{ArticuloSelection.Categoria.Descripcion}}</label>
                    </div>
                    <div class="col-md-2 col-sm-12 form-group fancy-form2">
